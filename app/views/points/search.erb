<table class="table-hover">
  <colgroup>
    <col class="1/3"></col>
    <col class="2/3"></col>
  </colgroup>
  <thead>
    <tr>
      <th>Search results for "<%= params[:term] %>":</th>
    </tr>
  </thead>
  <tbody>
    <%- @search_results.each do |point| %>
      <tr>
        <td><%= link_to point, edit_point_path(point) %></td>
        <td class="text-muted text-small"><%= point.inline_categories %></td>
      </tr>
    <% end %>
  </tbody>
</table>
